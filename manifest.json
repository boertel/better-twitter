{
    "manifest_version": 2,
    "name": "Better Twitter",
    "version": "0.1",
    "content_scripts": [
        {
            "matches": [
                "https://twitter.com/*"
            ],
            "js": [
                "jquery-3.2.1.min.js",
                "better-twitter.js"
            ],
            "css": [
                "better-twitter.css"
            ],
            "run_at": "document_end"
        }
    ]
}
